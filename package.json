{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -rf public/build && rollup -c",
    "postbuild": "workbox generateSW workbox-config.js",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "fix": "npx eslint --fix \"./src/**/*.{js,svelte}\"",
    "format": "npx prettier --write \"./src/**/*.{js,svelte}\""
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-plugin-svelte3": "^3.0.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.4.2",
    "rollup": "^2.35.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.0",
    "workbox-cli": "^6.0.2"
  },
  "dependencies": {
    "core-js": "^3.8.1",
    "d3": "^6.3.1",
    "sirv-cli": "^1.0.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,svelte}": ["eslint --cache --fix", "prettier --write"]
  }
}
